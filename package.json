{
  "name": "@readme/httpsnippet",
  "version": "8.1.4",
  "description": "HTTP Request snippet generator for *most* languages",
  "homepage": "https://github.com/readmeio/httpsnippet",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./helpers/code-builder": {
      "require": "./dist/helpers/code-builder.cjs",
      "import": "./dist/helpers/code-builder.js"
    },
    "./helpers/reducer": {
      "require": "./dist/helpers/reducer.cjs",
      "import": "./dist/helpers/reducer.js"
    },
    "./targets": {
      "require": "./dist/targets/index.cjs",
      "import": "./dist/targets/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.cts",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "api",
    "clojure",
    "csharp",
    "curl",
    "go",
    "har",
    "http",
    "httpie",
    "httr",
    "java",
    "javascript",
    "jquery",
    "kotlin",
    "objc",
    "objective-c",
    "ocaml",
    "php",
    "python",
    "request",
    "requests",
    "ruby",
    "shell",
    "snippet",
    "swift",
    "swift",
    "unirest",
    "xhr",
    "xmlhttprequest"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/readmeio/httpsnippet.git"
  },
  "scripts": {
    "attw": "attw --pack --format table-flipped",
    "build": "tsup",
    "clean": "rm -rf dist/",
    "lint": "eslint . --ext .js,.cjs,.ts",
    "prebuild": "npm run clean",
    "prepack": "npm run build",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write .",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "formdata-to-string": "^2.0.2",
    "qs": "^6.11.2",
    "stringify-object": "^3.3.0"
  },
  "devDependencies": {
    "@readme/eslint-config": "^13.4.0",
    "@types/eslint": "^8.44.7",
    "@types/har-format": "^1.2.15",
    "@types/node": "^20.9.0",
    "@types/qs": "^6.9.10",
    "@types/stringify-object": "^4.0.5",
    "@vitest/coverage-v8": "^0.34.4",
    "eslint": "^8.53.0",
    "prettier": "^3.0.3",
    "require-directory": "^2.1.1",
    "tsup": "^7.2.0",
    "type-fest": "^4.6.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  },
  "prettier": "@readme/eslint-config/prettier"
}
